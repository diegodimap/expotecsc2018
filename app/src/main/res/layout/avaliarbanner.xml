<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/tituloBanner"
        android:layout_width="0dp"
        android:layout_height="82dp"
        android:layout_marginBottom="11dp"
        android:layout_marginEnd="9dp"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="9dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="1dp"
        android:text="TextView"
        android:textColor="@color/zxing_viewfinder_laser"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/buttonSalvarNota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:onClick="salvarNotaBanner"
        android:text="AVALIAR BANNER"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout3" />

    <TableLayout
        android:id="@+id/tableLayout3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonSalvarNota"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tituloBanner">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/atitudeText"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="ATITUDE"
                android:textAlignment="center"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="217dp" />

            <EditText
                android:id="@+id/atitudeEdit"
                android:layout_width="match_parent"
                android:layout_height="40dp"

                android:ems="10"
                android:gravity="center_vertical"
                android:inputType="number"
                android:textAlignment="center"
                tools:layout_editor_absoluteX="206dp"
                tools:layout_editor_absoluteY="202dp" />

            <ImageView
                android:id="@+id/atitudeImage"
                android:layout_width="24dp"
                android:layout_height="match_parent"
                android:onClick="clicouAtitude"
                app:srcCompat="@mipmap/clipart1510606249"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="255dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/habilidadesText"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="HABILIDADES"
                android:textAlignment="center"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="217dp" />

            <EditText
                android:id="@+id/habilidadesEdit"
                android:layout_width="match_parent"
                android:layout_height="40dp"

                android:ems="10"
                android:gravity="center_vertical"
                android:inputType="number"
                android:textAlignment="center"
                tools:layout_editor_absoluteX="206dp"
                tools:layout_editor_absoluteY="202dp" />

            <ImageView
                android:id="@+id/habilidadesImage"
                android:layout_width="24dp"
                android:layout_height="match_parent"
                android:onClick="clicouHabilidades"
                app:srcCompat="@mipmap/clipart1510606249"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="255dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/criatividadeText"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
